<?xml version="1.0" encoding="UTF-8"?>
<content-type>
    <display-name>Contact Me Information</display-name>
    <content-display-name-script>$('name')</content-display-name-script>
    <super-type>base:unstructured</super-type>
    <is-abstract>false</is-abstract>
    <is-final>true</is-final>
    <is-built-in>false</is-built-in>
    <allow-child-content>true</allow-child-content>
    <form>
        <field-set name="basic">
            <label>Details</label>
            <items>
                <input type="TextLine" name="name">
                    <label>Name</label>
                    <immutable>false</immutable>
                    <indexed>true</indexed>
                    <occurrences minimum="0" maximum="1"/>
                </input>

                <input type="TextLine" name="email">
                    <label>Email</label>
                    <immutable>false</immutable>
                    <indexed>true</indexed>
                    <occurrences minimum="0" maximum="1"/>
                </input>

                <input type="TextLine" name="message">
                    <label>Message</label>
                    <immutable>false</immutable>
                    <indexed>true</indexed>
                    <occurrences minimum="0" maximum="1"/>
                </input>
                <input type="TextLine" name="phone">
                    <label>Phone</label>
                    <immutable>false</immutable>
                    <indexed>true</indexed>
                    <occurrences minimum="0" maximum="1"/>
                </input>
            </items>
        </field-set>

    </form>
</content-type>